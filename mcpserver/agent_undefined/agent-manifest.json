{
  "name": "agent_undefined",
  "displayName": "Undefined工具集",
  "version": "2.0.0",
  "description": "提供46个实用工具，包括热搜榜、音乐搜索、B站相关、AI绘图、文件操作、网络工具、消息操作等功能",
  "author": "NagaAgent",
  "agentType": "mcp",
  "entryPoint": {
    "module": "mcpserver.agent_undefined.agent_undefined",
    "class": "AgentUndefined"
  },
  "capabilities": {
    "invocationCommands": [
      {
        "command": "baiduhot",
        "description": "获取百度热搜榜；参数：limit（数量，默认10）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"baiduhot\",\"limit\":10}"
      },
      {
        "command": "weibohot",
        "description": "获取微博热搜榜；参数：limit（数量，默认10）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"weibohot\",\"limit\":10}"
      },
      {
        "command": "douyinhot",
        "description": "获取抖音热榜；参数：limit（数量，默认10）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"douyinhot\",\"limit\":10}"
      },
      {
        "command": "news_tencent",
        "description": "获取腾讯新闻；参数：page（数量）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"news_tencent\",\"page\":10}"
      },
      {
        "command": "renjian",
        "description": "获取人间日报（幽默句子）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"renjian\"}"
      },
      {
        "command": "history",
        "description": "获取历史上的今天",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"history\"}"
      },
      {
        "command": "bilibili_search",
        "description": "搜索B站视频；参数：msg（搜索内容）、n（数量）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"bilibili_search\",\"msg\":\"原神\",\"n\":5}"
      },
      {
        "command": "bilibili_user_info",
        "description": "获取B站用户信息；参数：mid（用户ID）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"bilibili_user_info\",\"mid\":\"123456\"}"
      },
      {
        "command": "music_global_search",
        "description": "全网音乐搜索；参数：msg（歌曲名）、n（序号）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"music_global_search\",\"msg\":\"稻香\",\"n\":1}"
      },
      {
        "command": "music_info_get",
        "description": "获取音乐详细信息；参数：id（音乐ID）、type（平台）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"music_info_get\",\"id\":\"123\",\"type\":\"qq\"}"
      },
      {
        "command": "music_lyrics",
        "description": "获取歌词；参数：id（音乐ID）、msg（平台）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"music_lyrics\",\"id\":\"123\",\"msg\":\"qq\"}"
      },
      {
        "command": "ai_draw_one",
        "description": "AI绘图（Midjourney）；参数：prompt（提示词）、model、size、target_id、message_type",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"ai_draw_one\",\"prompt\":\"一只可爱的猫咪\"}"
      },
      {
        "command": "local_ai_draw",
        "description": "本地AI绘图；参数：prompt（提示词）、negative_prompt、width、height、steps、cfg_scale、model",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"local_ai_draw\",\"prompt\":\"一只可爱的猫咪\",\"width\":512,\"height\":512}"
      },
      {
        "command": "ai_study_helper",
        "description": "AI学习助手；参数：question（问题）、content（上下文）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"ai_study_helper\",\"question\":\"什么是量子力学\"}"
      },
      {
        "command": "analyze_multimodal",
        "description": "多模态分析；参数：media_url（媒体URL）、media_type（类型）、prompt（提示词）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"analyze_multimodal\",\"media_url\":\"https://example.com/image.jpg\",\"media_type\":\"image\",\"prompt\":\"描述这张图片\"}"
      },
      {
        "command": "novel_search",
        "description": "小说搜索；参数：name（小说名）、href（链接）、num（数量）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"novel_search\",\"name\":\"三体\"}"
      },
      {
        "command": "read_file",
        "description": "读取文件；参数：file_path（文件路径）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"read_file\",\"file_path\":\"/path/to/file.txt\"}"
      },
      {
        "command": "search_file_content",
        "description": "搜索文件内容；参数：pattern（关键词）、path（路径）、include（文件类型）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"search_file_content\",\"pattern\":\"test\",\"path\":\"/path/to/dir\",\"include\":\"*.txt\"}"
      },
      {
        "command": "list_directory",
        "description": "列出目录；参数：path（路径）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"list_directory\",\"path\":\"/path/to/dir\"}"
      },
      {
        "command": "glob",
        "description": "文件模式匹配；参数：pattern（模式）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"glob\",\"pattern\":\"*.py\"}"
      },
      {
        "command": "base64",
        "description": "Base64编解码；参数：text（文本）、operation（encode/decode）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"base64\",\"text\":\"hello\",\"operation\":\"encode\"}"
      },
      {
        "command": "hash",
        "description": "哈希计算；参数：text（文本）、algorithm（算法，如md5/sha256）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"hash\",\"text\":\"hello\",\"algorithm\":\"md5\"}"
      },
      {
        "command": "gold_price",
        "description": "获取黄金价格",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"gold_price\"}"
      },
      {
        "command": "horoscope",
        "description": "星座运势；参数：constellation（星座）、time_type（类型）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"horoscope\",\"constellation\":\"双子座\"}"
      },
      {
        "command": "wenchang_dijun",
        "description": "文昌帝君抽签",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"wenchang_dijun\"}"
      },
      {
        "command": "tcping",
        "description": "TCP连通性测试；参数：address（地址）、port（端口）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"tcping\",\"address\":\"example.com\",\"port\":80}"
      },
      {
        "command": "speed",
        "description": "网站测速；参数：url（网址）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"speed\",\"url\":\"https://example.com\"}"
      },
      {
        "command": "net_check",
        "description": "全方位网络检测；参数：host（主机）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"net_check\",\"host\":\"example.com\"}"
      },
      {
        "command": "whois",
        "description": "域名查询；参数：domain（域名）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"whois\",\"domain\":\"example.com\"}"
      },
      {
        "command": "get_picture",
        "description": "获取各类图片；参数：message_type（消息类型）、target_id（目标ID）、picture_type（类型）、count（数量）、device、fourk_type",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"get_picture\",\"picture_type\":\"random\",\"count\":1}"
      },
      {
        "command": "minecraft_skin",
        "description": "Minecraft皮肤；参数：name（玩家名）、type、overlay、size、scale、target_id、message_type",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"minecraft_skin\",\"name\":\"Steve\",\"type\":\"skin\"}"
      },
      {
        "command": "video_random_recommend",
        "description": "随机视频推荐；参数：target_id（目标ID）、message_type（消息类型）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"video_random_recommend\"}"
      },
      {
        "command": "send_message",
        "description": "发送群聊消息；参数：message（消息）、at_user（艾特用户）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"send_message\",\"message\":\"大家好\",\"at_user\":\"123456\"}"
      },
      {
        "command": "send_private_message",
        "description": "发送私聊消息；参数：user_id（用户ID）、message（消息）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"send_private_message\",\"user_id\":\"123456\",\"message\":\"你好\"}"
      },
      {
        "command": "get_recent_messages",
        "description": "获取历史消息；参数：chat_id（聊天ID）、type（类型）、start（起始）、end（结束）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"get_recent_messages\",\"chat_id\":\"123456\",\"type\":\"private\"}"
      },
      {
        "command": "get_messages_by_time",
        "description": "按时间检索消息；参数：chat_id（聊天ID）、type（类型）、start_time（开始时间）、end_time（结束时间）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"get_messages_by_time\",\"chat_id\":\"123456\",\"type\":\"private\",\"start_time\":\"2024-01-01\",\"end_time\":\"2024-01-31\"}"
      },
      {
        "command": "get_forward_msg",
        "description": "获取转发消息详情；参数：message_id（消息ID）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"get_forward_msg\",\"message_id\":\"123456\"}"
      },
      {
        "command": "save_memory",
        "description": "保存记忆；参数：fact（事实）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"save_memory\",\"fact\":\"用户喜欢猫咪\"}"
      },
      {
        "command": "render_and_send_image",
        "description": "渲染LaTeX/Markdown为图片；参数：content（内容）、format（格式）、target_id（目标ID）、message_type（消息类型）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"render_and_send_image\",\"content\":\"E=mc^2\",\"format\":\"latex\"}"
      },
      {
        "command": "render_html",
        "description": "渲染HTML为图片；参数：html_content（HTML内容）、target_id（目标ID）、message_type（消息类型）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"render_html\",\"html_content\":\"<h1>Hello</h1>\"}"
      },
      {
        "command": "qq_level_query",
        "description": "QQ等级查询；参数：qq（QQ号）、uin、skey、pskey",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"qq_level_query\",\"qq\":\"123456789\"}"
      },
      {
        "command": "qq_like",
        "description": "QQ点赞；参数：target_user_id（目标用户ID）、times（次数）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"qq_like\",\"target_user_id\":\"123456\",\"times\":1}"
      },
      {
        "command": "read_naga_intro",
        "description": "Naga项目介绍文档",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"read_naga_intro\"}"
      },
      {
        "command": "debug",
        "description": "调试信息记录；参数：content（内容）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"debug\",\"content\":\"调试信息\"}"
      },
      {
        "command": "end",
        "description": "结束对话；参数：summary（总结）",
        "example": "{\"agentType\":\"mcp\",\"service_name\":\"agent_undefined\",\"tool_name\":\"end\",\"summary\":\"对话已结束\"}"
      }
    ]
  }
}
